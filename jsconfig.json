{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@components/*": ["components/*"],
      "@styles/*": ["styles/*"],
      "@utils/*": ["utils/*"],
      "@public/*": ["../public/*"],
      "@lib/*": ["lib/*"]
    },
    "skipLibCheck": true
  },
  "include": ["src", "scripts"]
}
